    "chest": {
      "aorta": {
        "description": "Measurement of pediatric thoracic aorta from contrast enhanced axial CT",
        "reference": "Fitzgerald SW, Donaldson JS, Poznanski AK. Pediatric thoracic aorta: Normal measurements determined with CT. Radiology 1987; 165:667-669",
        "measurement_levels": {
          "level_a": {
            "description": "1 cm below aortic arch",
            "formulas": {
              "a1": "0.72 × age + 11.55 mm",
              "a2": "0.629 × age + 9.54 mm"
            }
          },
          "level_b": {
            "description": "1 cm cranial to aortic root", 
            "formulas": {
              "b1": "0.668 × age + 13 mm",
              "b2": "0.572 × age + 9.56 mm"
            }
          },
          "level_c": {
            "description": "1 cm cranial to dome of right hemidiaphragm",
            "formula": "0.559 × age + 8.44 mm"
          }
        }
      },
      "bronchi": {
        "bronchiectasis_definition": "Bronchus-to-artery ratio > 1.2 on chest radiographs",
        "normal_ct_finding": "Normal bronchi are not usually seen in the most peripheral 5 to 10 mm of the lung on CT",
        "references": [
          "Woodring JH. Pulmonary artery-bronchus ratios in patients with normal lungs, pulmonary vascular plethora, and congestive heart failure. Radiology 1991; 179:115-22",
          "Siegel MJ. Pediatric Body CT. Lippincott Williams & Wilkins: Philadelphia. 1999. p. 101"
        ]
      },
      "cardiothoracic_index": {
        "description": "Made on upright frontal view of chest at 72 inches during quiet breathing",
        "formula": "(MRD + MLD)/ID",
        "definitions": {
          "id": "Internal diameter of chest at level of right hemidiaphragm",
          "mrd": "Greatest perpendicular diameter from midline to right heart border",
          "mld": "Greatest perpendicular diameter from midline to left heart border"
        },
